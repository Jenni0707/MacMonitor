package com.topking.ftp.bean;

public class UserInfoBean {

	private String userName; // 用户昵称
	private String userId; // 木马标识
	private String userIcon; // 用户头像路径
	private String userFtpUrl; // 用户ftp地址
	private String userFtpName; // 用户发图片账号
	private String userFtpPasswd; // 用户ftp密码
	private String userLocalPath; // 用户本地路径

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getUserID() {
		return userId;
	}

	public void setUserID(String uid) {
		this.userId = uid;
	}

	public String getUserIcon() {
		return userIcon;
	}

	public void setUserIcon(String uIcon) {
		this.userIcon = uIcon;
	}

	public String getFtpUrl() {
		return userFtpUrl;
	}

	public void setFtpUrl(String userFtpUrl) {
		this.userFtpUrl = userFtpUrl;
	}

	public String getFtpName() {
		return userFtpName;
	}

	public void setFtpName(String userFtpName) {
		this.userFtpName = userFtpName;
	}

	public String getFtpPasswd() {
		return userFtpPasswd;
	}

	public void setFtpPasswd(String userFtpPasswd) {
		this.userFtpPasswd = userFtpPasswd;
	}

	public String getLocalPath() {
		return userLocalPath;
	}

	public void setLocalPath(String userLocalPath) {
		this.userLocalPath = userLocalPath;
	}
}

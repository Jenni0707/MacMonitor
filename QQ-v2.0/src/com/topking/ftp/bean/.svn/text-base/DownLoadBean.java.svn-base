package com.topking.ftp.bean;public class DownLoadBean {	private FileBean fb;	private String localPath;	private int rowIndex;	private String remotePath;	public FileBean getFb() {		return fb;	}	public void setFb(FileBean fb) {		this.fb = fb;	}	public String getLocalPath() {		return localPath;	}	public void setLocalPath(String localPath) {		this.localPath = localPath;	}	public String getRemotePath() {		return remotePath;	}	public String setRemotePath(String remotePath) {		return this.remotePath = remotePath;	}	public int getRowIndex() {		return rowIndex;	}	public void setRowIndex(int rowIndex) {		this.rowIndex = rowIndex;	}}